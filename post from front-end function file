<?php
// submit post from prontend
if (isset($_POST['title']) ) {

  $title = $_POST['title'];
  $desc = $_POST['desc'];
  $thumb = $_POST['test'];

  $my_post =array(
    'post_type' =>'testimonials',
    'post_title' => $title,
    'post_content' => $desc,
    'post_status' => 'publish',
     
);
  
  wp_insert_post($my_post);

}

